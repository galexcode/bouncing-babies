package com.wastedpotential {	import flash.geom.Point;	import flash.display.MovieClip;		public class ArcMaker extends MovieClip {		public function ArcMaker() {			//trace("ArcMaker :: CONSTRUCTOR");					var point1:Point = new Point(-0, 75);			var point2:Point = new Point(16, 75);			var point3:Point = new Point(32, 75);			drawArc(point1, point2, point3, 0);						point1 = new Point(-35, 290);			point2 = new Point(32, 75);			point3 = new Point(109, 290);			drawArc(point1, point2, point3, 1);						point1 = new Point(109, 290);			point2 = new Point(166, 150);			point3 = new Point(233, 290);			drawArc(point1, point2, point3, 2);						point1 = new Point(233, 290);			point2 = new Point(300, 200);			point3 = new Point(367, 290);			drawArc(point1, point2, point3, 3);						point1 = new Point(367, 290);			point2 = new Point(434, 230);			point3 = new Point(501, 290);			drawArc(point1, point2, point3, 4);						function drawArc(point1:Point, point2:Point, point3:Point, index:int) {				var xOffset:Number = point1.x;				point1.x = point1.x - xOffset;				point2.x = point2.x - xOffset;				point3.x = point3.x - xOffset;								var C:Number = point1.y;				var A:Number = (point3.x*(point2.y - point1.y) + point2.x*(point1.y - point3.y))/(point2.x*point3.x*(point2.x - point3.x));				var B:Number = (point3.y - point1.y)/point3.x - point3.x*A;								for (var i:int=point1.x; i<=point3.x; i++) {					var myy:Number = A*i*i + B*i + C;					var mydot:MovieClip = new dot();					mydot.x = i + xOffset;					mydot.y = myy;					addChild(mydot);				}				trace("private var xOffset"+index+":int = "+xOffset+";");				trace("private var A"+index+":Number = "+A+";");				trace("private var B"+index+":Number = "+B+";");				trace("private var C"+index+":Number = "+C+";");				trace(" ");			}		}	}	}