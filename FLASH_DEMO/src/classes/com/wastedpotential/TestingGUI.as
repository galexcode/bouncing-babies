package com.wastedpotential {	import com.bit101.components.FPSMeter;	import com.bit101.components.InputText;	import com.bit101.components.Label;	import com.bit101.components.Panel;	import com.bit101.components.Text;	import com.bit101.components.Window;	import com.bit101.components.PushButton;	import flash.events.Event;	import flash.events.MouseEvent;		//- sets up the minimalcomps testing panel	public class TestingGUI extends Panel {				private var fpsMeter:FPSMeter;		private var multiplierLabel:Label;		private var multiplierSetter:InputText;						//- set up the testing panel		public function TestingGUI() {			//trace("TestingGUI :: CONSTRUCTOR");						super(null, 0, 0); 			width = 130;			height = 160;						fpsMeter = new FPSMeter(this, 5, 5, "FPS = ");			fpsMeter.start();						multiplierLabel = new Label(this, 5, 55, "Speed Multiplier");			multiplierSetter = new InputText(this, 80, 55, String(Settings.speedMultiplier), onMultiplierChange);			multiplierSetter.width = 40;			multiplierSetter.height = 22;		}								//- input handler - changes the game gravity		private function onMultiplierChange(e:Event):void {			//trace("TestingGUI :: onGravityChange");			var myValue:Number = Number(multiplierSetter.text);			//if (myValue < 0.5) myValue = 0.5;			//if (myValue > 5) myValue = 5;			Settings.speedMultiplier = myValue;			//multiplierSetter.text = myValue+"";		}					} //end class} //end package