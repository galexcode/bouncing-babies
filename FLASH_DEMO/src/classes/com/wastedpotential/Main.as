package com.wastedpotential {	import flash.display.MovieClip;	import flash.events.Event;		public class Main extends MovieClip {				private var currentX:Number;		private var currentY:Number;		private var vX:Number = 2;		private var baby:MovieClip;				private var xOffset1:int = -35;		private var y1:Number = 300;		private var v1:Number = -6.2803;		private var a1:Number = 0.04361;				private var xOffset2:int = 109;		private var y2:Number = 300;		private var v2:Number = -4.8703;		private var a2:Number = 0.03928;				private var xOffset3:int = 233;		private var y3:Number = 300;		private var v3:Number = -2.9851;		private var a3:Number = 0.02228;				private var xOffset4:int = 367;		private var y4:Number = 300;		private var v4:Number = -2.0896;		private var a4:Number = 0.01559;				private const START_X:int = 32;		private const FINAL_X:int = 500;		//- constructor		public function Main() {			trace("Main :: CONSTRUCTOR");			currentX = START_X;			baby = new Dot();			addChild(baby);			baby.x = currentX;			addEventListener(Event.ENTER_FRAME, onFrame);		}				private function onFrame(e:Event=null):void {			currentX = currentX + vX;			if (currentX > FINAL_X) { //loop it				currentX = START_X;			}			var xOffset:Number;			var y0:Number;			var v0:Number;			var a0:Number;			if (currentX < xOffset2) {				xOffset = xOffset1;				y0 = y1;				v0 = v1;				a0 = a1;			}			else if (currentX < xOffset3) {				xOffset = xOffset2;				y0 = y2;				v0 = v2;				a0 = a2;			}			else if (currentX < xOffset4) {				xOffset = xOffset3;				y0 = y3;				v0 = v3;				a0 = a3;			}						else { //the last bounce segment before it loops				xOffset = xOffset4;				y0 = y4;				v0 = v4;				a0 = a4;			}			currentY = y0 + v0*(currentX - xOffset) + a0*(currentX - xOffset)*(currentX - xOffset);			baby.x = currentX;			baby.y = currentY;		}	}	}